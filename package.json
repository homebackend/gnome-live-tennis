{
  "name": "gnome-live-tennis",
  "version": "3.0.0",
  "description": "Gnome Extension to show live tennis data",
  "homepage": "https://github.com/homebackend/gnome-live-tennis#readme",
  "bugs": {
    "url": "https://github.com/homebackend/gnome-live-tennis/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/homebackend/gnome-live-tennis.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "neerajcd@gmail.com",
  "type": "module",
  "main": "run/main.cjs",
  "scripts": {
    "build": "node esbuild.js",
    "start": "npm run start:electron",
    "start:electron": "npm run build && ./run-electron.sh",
    "prettify": "prettier --write dist/extension.js dist/prefs.js",
    "generate:schemas": "npm run build && mkdir -p dist/schemas && node dist/schema.js",
    "compile:schemas": "npm run generate:schemas && glib-compile-schemas dist/schemas/",
    "package": "./package.sh",
    "deploy": "./deploy.sh",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@girs/adw-1": "^1.8.0-4.0.0-beta.36",
    "@girs/clutter-16": "^16.0.0-4.0.0-beta.38",
    "@girs/gdkpixbuf-2.0": "^2.0.0-4.0.0-beta.38",
    "@girs/gio-2.0": "^2.86.0-4.0.0-beta.38",
    "@girs/gjs": "^4.0.0-beta.36",
    "@girs/glib-2.0": "*",
    "@girs/gnome-shell": "^48.0.4",
    "@girs/gobject-2.0": "*",
    "@girs/gtk-4.0": "^4.19.1-4.0.0-beta.36",
    "@girs/meta-16": "^16.0.0-4.0.0-beta.38",
    "@girs/soup-3.0": "^3.6.5-4.0.0-beta.36",
    "@girs/st-16": "^16.0.0-4.0.0-beta.38",
    "@types/electron": "^1.4.38",
    "electron": "^38.4.0",
    "esbuild": "^0.25.10",
    "prettier": "^3.6.2",
    "sslkeylog": "^0.2.2",
    "typescript": "^5.9.3",
    "xml-js": "^1.6.11"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "electron-store": "^11.0.2"
  }
}
